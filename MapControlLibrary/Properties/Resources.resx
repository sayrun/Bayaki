  <data name="googlemapsHTML" xml:space="preserve">
    <value>&lt;html&gt;
&lt;script type="text/javascript" src="http://maps.google.com/maps/api/js?key=[[KEY]]&amp;sensor=false&amp;v=3.22"&gt;&lt;/script&gt;
&lt;script type="text/javascript"&gt;
var map;
var marker = null;
var path = null;
var count = 0;
var drawLine = null;

function Initialize() {
	var result = false;
	try{
		var y = 34.784167;
		var x = 135.400806;
		var latlng = new google.maps.LatLng(y, x);
		var opts = {
			zoom: 14,
			center: latlng,
			streetViewControl: false,
			mapTypeId: google.maps.MapTypeId.ROADMAP
		};
		map = new google.maps.Map(document.getElementById("gmap"), opts);

		result = true;
	}catch(e)
	{
		// エラーは戻り値で通知するようにしてみる。
	}
	return result.toString();
}

function movePos( lat, lon) {
	var result = false;
	try{
		var pos = new google.maps.LatLng(lat, lon);

		if( null != marker)
		{
			marker.setMap(null);
			marker = null;
		}

		marker = new google.maps.Marker( { map: map, position: pos } );
		marker.setDraggable(true);
		google.maps.event.addListener(marker, "dragend", function(e){
			var latlon = marker.getPosition();
			window.external.notifyLatLon( latlon.lat(), latlon.lng());
		});
		marker.setMap(map);
		map.setCenter(pos);

		result = true;
	}catch(e)
	{
		// エラーは戻り値で通知するようにしてみる。
	}
	return result.toString();
}

function resetMarker() {
	var result = false;
	try{
		if( null != marker) {
			marker.setMap(null);
			marker = null;
		}

		result = true;
	}catch(e)
	{
		// エラーは戻り値で通知するようにしてみる。
	}
	return result.toString();
}

function resizeMap() {
	var result = false;
	try{
		var center = map.getCenter();
		google.maps.event.trigger(map, 'resize');
		map.setCenter(center);

		result = true;
	}catch(e)
	{
		// エラーは戻り値で通知するようにしてみる。
	}
	return result.toString();
}

function addPoint( lat, lon, interest) {
	var result = false;
	try{
		path[count] = new google.maps.LatLng(lat, lon);
		intersts[count] = interest;
		count++;

		result = true;
	}catch(e)
	{
		// エラーは戻り値で通知するようにしてみる。
	}
	return result.toString();
}

function drawPolyline() {
	var result = false;
	try{
		if(null != drawLine)
		{
			drawLine.setMap(null);
			drawLine = null;
		}

		drawLine = new google.maps.Polyline({
			path: path,
			strokeColor: '#8855FF',
			strokeOpacity: 0.8,
			strokeWeight: 4
		});

		var bounds = new google.maps.LatLngBounds();
		for( index = 0; index &lt; count; ++index) {
			if( "" != intersts[index]) {
				marker = new google.maps.Marker( { map: map, position: path[ index], title: intersts[index]} );
				marker.setMap(map);
			}
			bounds.extend(path[ index]);
		}

		drawLine.setMap(map);
		map.fitBounds (bounds);

		result = true;
	}catch(e)
	{
		// エラーは戻り値で通知するようにしてみる。
	}
	return result.toString();
}

function clearPoint() {
	var result = false;
	try{
		path = [];
		intersts = [];
		count = 0;

		result = true;
	}catch(e)
	{
		// エラーは戻り値で通知するようにしてみる。
	}
	return result.toString();
}

function dropMarker() {
	var result = false;
	try{
		if( null == marker) {
			var latlon = map.getCenter();
			marker = new google.maps.Marker( { map: map, position: latlon } );
		
			window.external.notifyLatLon( latlon.lat(), latlon.lng());
		}

		marker.setDraggable(true);
		google.maps.event.addListener(marker, "dragend", function(e){
			var latlon = marker.getPosition();
			window.external.notifyLatLon( latlon.lat(), latlon.lng());
		});

		result = true;
	}catch(e)
	{
		// エラーは戻り値で通知するようにしてみる。
	}
	return result.toString();
}
&lt;/script&gt;
&lt;body topmargin="0" leftmargin="0" bottommargin="0" rightmargin="0"&gt;
&lt;div id="gmap" style="width:100%; height:100%"&gt;&lt;/div&gt;
&lt;/body&gt;
&lt;/html&gt;</value>
